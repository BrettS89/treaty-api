{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["./*"]
    }
  },
  "exclude": [
    "test"
  ]
}
